{
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "NETCORE_SUPPORT", "NETCORE", "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "NETCORE_SUPPORT", "NETCORE", "TRACE" ],
        "optimize": true
      }
    }
  },
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": true
  },
  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.1.0"
    },
    "NUnitLite": "3.5.0",
    "ServiceStack.OrmLite": "1.0.*",
    "ServiceStack.Text": "1.0.*",
    "ServiceStack.Client": "1.0.*",
    "ServiceStack.OrmLite.Sqlite": "1.0.*",
    "ServiceStack.OrmLite.SqlServer": "1.0.*",
    "ServiceStack.OrmLite.PostgreSQL": "1.0.*",
    "ServiceStack.OrmLite.MySql": "1.0.*"
  },
  "frameworks": {
    "netcoreapp1.1": {
      "imports": "dnxcore50",
      "dependencies": {
        "System.Runtime": "4.*",
        "System.Runtime.Serialization.Xml": "4.*",
        "System.Reflection": "4.*",
        "System.Reflection.Primitives": "4.*",
        "System.Runtime.Serialization.Primitives": "4.*",
        "System.Reflection.TypeExtensions": "4.*"
      }
    }
  }
}